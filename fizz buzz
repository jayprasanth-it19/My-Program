
#include<stdio.h>
void fizz_buzz(int x);
void main()
{
   int a;
 scanf("%d",&a);
 fizz_buzz(a);
}
void fizz_buzz(int x)
{
   int a;
   if(a%3==0 && a%5==0)
   {
       printf("fizzbuzz");
   }
   else if(a%3==0)
   {
       printf("fizz");
   }
   else if(a%5==0)
   {
       printf("buzz");
   }
   else
   {
       printf("%d",x);
   }
}
